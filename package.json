{
  "name": "cypress-v13-cucumber-example",
  "version": "1.1.0",
  "description": "Cypress v13 + Cucumber example project",
  "main": "index.js",
  "author": "ChristianAA",
  "license": "MIT",
  "scripts": {
    "execution": "npx cypress run --browser=chrome",
    "open": "npx cypress open"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "21.0.2",
    "@bahmutov/cypress-esbuild-preprocessor": "2.2.3",
    "@esbuild-plugins/node-modules-polyfill": "0.2.2",
    "cypress": "13.15.1"
  },
  "cypress-cucumber-preprocessor": {
    "omitFiltered": true,
    "stepDefinitions": [
        "cypress/e2e/step_definitions/**/*.{js,ts}"
    ],
    "messages": {
        "enabled": true,
        "output": "cypress/cucumber-results/cucumber-messages.ndjson"
    },
    "json": {
        "enabled": true,
        "output": "cypress/cucumber-results/cucumber.json"
    },
    "html": {
      "enabled": true,
      "output": "cypress/cucumber-results/report.html"
    }
  }
}