{
  "name": "cypress-v15-cucumber-example",
  "version": "1.2.0",
  "description": "Cypress v15 + Cucumber example project",
  "main": "index.js",
  "author": "ChristianAA",
  "license": "MIT",
  "scripts": {
    "execution": "npx cypress run --browser=chrome",
    "open": "npx cypress open"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "23.1.0",
    "@bahmutov/cypress-esbuild-preprocessor": "2.2.7",
    "esbuild-plugins-node-modules-polyfill": "1.7.1",
    "cypress": "15.5.0"
  },
  "cypress-cucumber-preprocessor": {
    "omitFiltered": true,
    "stepDefinitions": [
      "cypress/e2e/step_definitions/**/*.{js,ts}"
    ],
    "messages": {
      "enabled": true,
      "output": "cypress/cucumber-results/cucumber-messages.ndjson"
    },
    "json": {
      "enabled": true,
      "output": "cypress/cucumber-results/cucumber.json"
    },
    "html": {
      "enabled": true,
      "output": "cypress/cucumber-results/report.html"
    }
  }
}